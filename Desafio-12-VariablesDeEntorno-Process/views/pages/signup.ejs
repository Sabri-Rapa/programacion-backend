<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>

  <body>
    <div>
      <h1>REGISTRARSE</h1>

      <h3 id="div-compres"></h3>

      <form action="" method="post">
        <div id="login-div">
          <input
            type="text"
            id="username"
            name="username"
            value=""
            placeholder="Username"
          />
          <input
            type="password"
            id="password"
            name="password"
            value=""
            placeholder="Password"
            autocomplete="off"
          />
        </div>

        <div>
          <button type="submit" id="login-btn">Registrarse</button>
        </div>
      </form>

      <div>
        <div><br /></div>
        <button onclick="location.href='/api/login'">Volver a Login</button>
      </div>
    </div>
    <script src="/socket.io/socket.io.js"></script>
  </body>
</html>
