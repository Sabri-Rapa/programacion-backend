<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>

  <body onload="redirect()">
    <h1>Datos de proceso</h1>

    <table class="table">
      <thead>
        <tr>
          <th scope="col">Argumentos</th>
          <th scope="col">Path de ejecución</th>
          <th scope="col">Sistema Operativo</th>
          <th scope="col">Process ID</th>
          <th scope="col">Versión de Node</th>
          <th scope="col">Carpeta del proyecto</th>
          <th scope="col">Memoria</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <th scope="row">
            <% for (const ele in argumentos) { %>
            <p><%=ele%>: <%=argumentos[ele]%></p>
            <%}%>
          </th>
          <td><%= pathDeEjecucion %></td>

          <td><%= sistemaOperativo %></td>

          <td><%= idProceso %></td>

          <td><%= versionNode %></td>

          <td><%= carpetaProyecto %></td>
          <td>
            <% for (const ele in memoria) { %>
            <p><%=ele%>: <%=memoria[ele]%></p>
            <%}%>
          </td>
        </tr>
      </tbody>
    </table>
  </body>
</html>
